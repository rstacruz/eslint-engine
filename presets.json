{
  "standard": {
    "rc": {
      "extends": [
        "standard",
        "standard-jsx"
      ]
    },
    "npm": [
      "eslint@2",
      "eslint-config-standard@5",
      "eslint-config-standard-jsx@1",
      "eslint-plugin-promise",
      "eslint-plugin-react",
      "eslint-plugin-standard"
    ]
  },
  "standard-nojsx": {
    "rc": {
      "extends": [
        "standard"
      ]
    },
    "npm": [
      "eslint@2",
      "eslint-config-standard",
      "eslint-plugin-promise",
      "eslint-plugin-standard"
    ]
  },
  "semistandard": {
    "rc": {
      "extends": [
        "semistandard",
        "standard-jsx"
      ]
